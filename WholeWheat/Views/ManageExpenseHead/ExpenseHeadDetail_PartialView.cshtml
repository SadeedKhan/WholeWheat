@model List<WholeWheatRepository.Models.ManageExpenseHead>

<div class="row" style="margin-top:100px;">
    <table id="Table" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>Expense Head ID</th>
                <th>Expense Head Name</th>
                <th>Status</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var currentexpensehead in Model)
            {
                <tr>
                    <td>@currentexpensehead.ExpenseHeadID</td>
                    <td>@currentexpensehead.ExpenseHeadName</td>
                    <td>@currentexpensehead.Status</td>
                    <td>
                        <div class="btn-group">
                            <span data-expenseheadid="@currentexpensehead.ExpenseHeadID" data-expenseheadname="@currentexpensehead.ExpenseHeadName" data-StatusId="@currentexpensehead.StatusID" data-Status="@currentexpensehead.Status" data-target="#AddExpenseHead" data-toggle="modal" class="EditExpenseHead btn btn-default">
                                <i class="fa fa-edit"></i>
                                Edit
                            </span>
                        </div>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
<script src="~/js/app.js"></script>