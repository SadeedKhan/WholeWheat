@model List<WholeWheatRepository.Models.ManageExpenseName>

<div class="row" style="margin-top:100px;">
    <table id="Table" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>Expense Name ID</th>
                <th>Expense Name</th>
                <th>Status</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var currentexpensename in Model)
            {
                <tr>
                    <td>@currentexpensename.ExpenseNameID</td>
                    <td>@currentexpensename.ExpenseName</td>
                    <td>@currentexpensename.Status</td>
                    <td>
                        <div class="btn-group">
                            <span data-expensenameid="@currentexpensename.ExpenseNameID" data-expensename="@currentexpensename.ExpenseName" data-StatusId="@currentexpensename.StatusID" data-Status="@currentexpensename.Status" data-target="#AddExpenseName" data-toggle="modal" class="EditExpenseName btn btn-default">
                                <i class="fa fa-edit"></i>
                                Edit
                            </span>
                        </div>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
<script src="~/js/app.js"></script>
