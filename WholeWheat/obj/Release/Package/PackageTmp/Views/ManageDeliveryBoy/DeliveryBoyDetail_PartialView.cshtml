@model List<WholeWheatRepository.Models.ManageDeliveryBoy>

<div class="row" style="margin-top:100px;">
    <table id="Table" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>DeliveryBoy Name</th>
                <th>Ceated At</th>
                <th>Phone</th>
                <th class="hidden-xs">Email</th>
                <th class="hidden-xs">Cnic</th>
                <th class="hidden-xs">Description</th>
                <th class="hidden-xs">Address</th>
                <th class="hidden-xs">Status</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var currentdeliveryboy in Model)
            {
                <tr>
                    <td>@currentdeliveryboy.DeliveryBoyName</td>
                    <td>@currentdeliveryboy.Date</td>
                    <td>@currentdeliveryboy.Phone</td>
                    <td>@currentdeliveryboy.Email</td>
                    <td>@currentdeliveryboy.Cnic</td>
                    <td>@currentdeliveryboy.Description</td>
                    <td>@currentdeliveryboy.Address</td>
                    <td>@currentdeliveryboy.Status</td>
                    <td>
                        <div class="btn-group">
                            <span data-DeliveryBoyid="@currentdeliveryboy.DeliveryBoyID" data-DeliveryBoyName="@currentdeliveryboy.DeliveryBoyName" data-Phone="@currentdeliveryboy.Phone" data-Email="@currentdeliveryboy.Email" data-Cnic="@currentdeliveryboy.Cnic" data-Description="@currentdeliveryboy.Description" data-Address="@currentdeliveryboy.Address" data-StatusId="@currentdeliveryboy.StatusID" data-Status="@currentdeliveryboy.Status" data-target="#AddDeliveryBoy" data-toggle="modal" class="EditDeliveryBoy btn btn-default">
                                <i class="fa fa-edit"></i>
                            </span>
                        </div>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
<script src="~/js/app.js"></script>

